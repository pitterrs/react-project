{
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      },
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jquery": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-console": 1,
      "no-undef": 0,
      "no-unused-vars": 1,
      "no-redeclare": 1,
      "no-cond-assign": 1,
      "no-empty": 1
    }
  }
  